{
  "name": "ngx-daterangepicker-material-daysjs",
  "version": "5.0.1",
  "description": "Angular 9 and 10 date range picker (with material design theme) using daysjs instead momentjs",
  "keywords": [
    "angular2",
    "angular4",
    "angular5",
    "angular6",
    "angular7",
    "angular8",
    "angular9",
    "angular10",
    "ng2",
    "daterangepicker",
    "material",
    "component",
    "directive",
    "daterange",
    "date",
    "dayjs",
    "picker"
  ],
  "homepage": "https://github.com/fetrarij/ngx-daterangepicker-material",
  "license": "MIT",
  "main": "dist/esm2015/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "demo:build": "ng build --prod",
    "test": "./node_modules/.bin/ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngc": "./node_modules/.bin/ngc -p tsconfig.json",
    "gh-pages": "ng build --prod --base-href=\"/ngx-daterangepicker-material/\" && ngh --dir demo/dist",
    "build": "ng build",
    "build-p": "ng build --configuration production",
    "build:schematics": "tsc -p ./tsconfig.schematics.json",
    "copy:schematics": "cp -r schematics/dist dist/schematics && cp -r schematics/collection.json dist/schematics && cp -r schematics/ng-add/schema.json dist/schematics/ng-add",
    "postbuild": "npm run build:schematics && npm run copy:schematics"
  },
  "author": "Fetrarijaona R.",
  "repository": {
    "type": "git",
    "url": "https://github.com/fetrarij/ngx-daterangepicker-material.git"
  },
  "peerDependencies": {
    "dayjs": "^1.10.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.3",
    "@angular/animations": "^13.3.5",
    "@angular/cdk": "^13.3.4",
    "@angular/cli": "~13.3.3",
    "@angular/common": "^13.3.5",
    "@angular/compiler": "^13.3.5",
    "@angular/compiler-cli": "~13.3.5",
    "@angular/core": "^13.3.5",
    "@angular/forms": "^13.3.5",
    "@angular/material": "^13.3.4",
    "@angular/platform-browser": "^13.3.5",
    "@angular/platform-browser-dynamic": "^13.3.5",
    "@angular/router": "^13.3.5",
    "@schematics/angular": "^13.3.3",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^17.0.29",
    "angular-cli-ghpages": "^1.0.0",
    "codelyzer": "^6.0.0",
    "core-js": "^3.22.2",
    "cpr": "^3.0.1",
    "jasmine-core": "~4.1.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.19",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "materialize-css": "^1.0.0",
    "ng-packagr": "^13.3.1",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "ts-node": "~10.7.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.3",
    "zone.js": "~0.11.5"
  },
  "dependencies": {
    "dayjs": "^1.11.1",
    "tslib": "^2.4.0"
  }
}
